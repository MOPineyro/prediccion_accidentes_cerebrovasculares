{
    "name": "stroke-prediction",
    "description": "Predicci√≥n de accidentes cerebrovasculares usando PyTorch",
    "image": "saturncloud/pytorch:latest",
    "git_repository": {
        "url": "git@github.com:MOPineyro/prediccion_accidentes_cerebrovasculares.git",
        "branch": "main"
    },
    "hardware": {
        "instance_type": "gpu-small",
        "disk_space": "10Gi"
    },
    "environment": {
        "pip_packages": "requirements.txt",
        "working_directory": "${WORKSPACE_DIR}",
        "setup_script": ".saturn/setup.sh",
        "env_vars": {
            "PYTHONPATH": "${PYTHONPATH}:${WORKSPACE_DIR}"
        }
    },
    "jupyter_server": {
        "auto_start": true,
        "auto_stop": "1h"
    }
} 